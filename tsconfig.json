{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@states/*": ["src/states/*"],
      "@helpers/*": ["src/helpers/*"],
      "@components/*": ["src/components/*"],
      "@mocks/*": ["src/mocks/*"],
      "@containers/*": ["src/containers/*"],
      "@database/*": ["src/database/*"],
      "@types/*": ["src/types/*"]
    }
  },
}
